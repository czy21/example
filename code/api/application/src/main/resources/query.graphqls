type Query{
    userDetail(id:String!):User
}

type Mutation{
    searchUser(input:PageUserInput):PageUserModel
    findAllUser:[User]
}

input UserInput{
    loginName:String
}

input PageUserInput{
    page:PageInput
    filter:[UserInput]
}

input PageInput{
    pageIndex:Int
    pageSize:Int
}

type PageDTO{
    pageIndex:Int
    pageSize:Int
    total:Int
}

type PageUserModel{
    page:PageDTO
    list:[User]
}