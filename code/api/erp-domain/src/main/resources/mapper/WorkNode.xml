<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.repository.mybatis.system.WorkNodeRepository">
    <resultMap id="workerNodeRes" type="com.team.entity.mybatis.system.WorkNode">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="launch_date" jdbcType="DATE" property="launchDate"/>
        <result column="sequence_value" jdbcType="BIGINT" property="sequenceValue"/>
		<result column="added_time" property="addedTime"/>
		<result column="modified_time" property="modifiedTime"/>
    </resultMap>

    <insert id="addWorkerNode" useGeneratedKeys="true" keyProperty="id"
            parameterType="com.team.entity.mybatis.system.WorkNode">
		INSERT INTO sys_work_node
		(launch_date,
		sequence_value,
		added_time,
		modified_time)
		VALUES (
		#{launchDate},
		#{sequenceValue},
		NOW(),
		NOW())
	</insert>
</mapper>