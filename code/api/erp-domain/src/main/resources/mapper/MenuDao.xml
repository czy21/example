<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.repository.mybatis.system.MenuRepositoryMybatis">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.team.entity.mybatis.system.Menu">
        <id column="menu_id" property="menuId" />
        <result column="added_time" property="addedTime" />
        <result column="modified_time" property="modifiedTime" />
        <result column="added_user" property="addedUser" />
        <result column="modified_user" property="modifiedUser" />
        <result column="parent_id" property="parentId" />
        <result column="menu_name" property="menuName" />
        <result column="icon" property="icon" />
        <result column="sort" property="sort" />
        <result column="url" property="url" />
        <result column="is_menu" property="isMenu" />
        <result column="remark" property="remark" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        added_time,
        modified_time,
        added_user,
        modified_user,
        menu_id
        parent_id
        menu_name
        icon
        sort
        url
        is_menu
        remark
    </sql>

</mapper>
