pipeline {
    agent any
    stages {
        stage('init') {
            steps{
                sh 'pip3 install -r script/requirements.txt'
            }
        }
        stage('build') {
            steps{
                parallel(
                    db:{
                        sh 'echo "build_db"'
                    },
                    api:{
                        sh 'echo "build_api"'
                    }
                )
            }
        }
    }
}
