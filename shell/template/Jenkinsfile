pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'build'
                echo $WORKSPACE
                echo $HOME
                pwd
            }
        }
        stage('Test'){
            steps {
                echo 'Test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy'
            }
        }
    }
}